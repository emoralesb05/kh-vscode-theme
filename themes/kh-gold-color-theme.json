  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#8B98AF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#E8C98A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#7FD3FF"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#8CF0C8"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#22D3EE",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#ECF4FF"
      }
    },
    {
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#F2D88B"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#ECF4FF",
    "parameter": "#F2D88B",
    "property": "#CFDAED",
